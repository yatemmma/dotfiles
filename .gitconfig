[user]
	name = yatemmma
	email = yatemmma@gmail.com
[alias]
        alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
	st = status -sb
	co = checkout
	b = branch
	ba = branch -a
	ci = commit -a
        amend = commit --amend
        l = log --color --pretty=\"format:%Cgreen%ai%Creset %h %Cred(%an):%Creset%C(yellow)%d%Creset %s\" -40
	ls = log --stat -p
	tree = log --graph --color --pretty=\"format:%Cgreen%ai%Creset %h %Cred(%an):%Creset%C(yellow)%d%Creset %s\" -20
	df = diff
	dfc = diff --cached
	dff = diff HEAD
	dfw = diff --color-words
        ft = fetch --prune
        rh = reset --hard
	bk = !git branch $1_bk
[color]
	ui = auto
[core]
	pager = less -r
